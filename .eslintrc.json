{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "linebreak-style": "off",
        "@typescript-eslint/strict-boolean-expressions": "off"
      }
}
